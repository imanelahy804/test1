@model List<listlibraryvm>
@{
    ViewData["Title"] = "لیست افراد";
}
<a class="btn btn-danger"asp-controller="Home" asp-action="create"  dir="rtl">ثبت نام</a>
<a class="btn btn-danger"asp-controller="Home" asp-action="searchbox"  dir="rtl"> جست و جو</a>
<table class="table  table-dark table-striped" dir="rtl">
  <thead>
    <tr>
      
      <th scope="col">شماره</th>
       <th scope="col">شناسه </th>
      <th scope="col">نام</th>
      <th scope="col">نام کتابخانه </th>
      <th scope="col">شهر و آدرس </th>
      <th scope="col">کتاب امانت </th>
     
       
      
    </tr>
  </thead>
  <tbody>
   
    @for (var i = 0; i < Model.Count; i++)
    {
        <tr>
        <td>@(i+1)</td>
         <td>@Model[i].ID</td>
      <td>@Model[i].memberName</td>
      <td>@Model[i].LibraryName</td>
      <td>@Model[i].CityAndAddres</td>
      <td>@Model[i].bookname</td>
      <td>
        </td>
      </tr>
       
        

     

     
    }
     
    
   
    
  </tbody>
</table>
